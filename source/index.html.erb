  <div>
  <div id="historyPane">
    <div>
      <p id="historyTitle">History</p>
      <p id="historySubtext">Click An Entry To Insert</p>
      <div id="bigHistory"></div>
    </div>
    <div id="scrollButtons">
      <p id=scrollUp>↑</p>
      <p id=scrollDown>↓</p>
    </div>
  </div>
  <div id=scrollToBottom>Return To Bottom</div>
  <div id="memoryValue"></div>
